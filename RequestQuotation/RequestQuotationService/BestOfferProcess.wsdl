<?xml version="1.0" encoding="UTF-8"?>
<wsdl:definitions
     name="BestOfferProcess"
     targetNamespace="http://xmlns.oracle.com/ObtainBestOffer/ObtainBestOfferService/BestOfferProcess"
     xmlns:ns1="http://bestoffer.truckhub.org"
     xmlns:plnk="http://docs.oasis-open.org/wsbpel/2.0/plnktype"
     xmlns:client="http://xmlns.oracle.com/ObtainBestOffer/ObtainBestOfferService/BestOfferProcess"
     xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/"
    >
    <plnk:partnerLinkType name="BestOfferProcess">
        <plnk:role name="BestOfferProcessProvider" portType="client:BestOfferProcess"/>
    </plnk:partnerLinkType>
    <wsdl:types>
        <schema xmlns="http://www.w3.org/2001/XMLSchema">
            <import namespace="http://bestoffer.truckhub.org" schemaLocation="xsd/BestOfferProcess.xsd"/>
        </schema>
    </wsdl:types>
    <wsdl:message name="BestOfferProcessRequestMessage">
        <wsdl:part name="payload" element="ns1:request"/>
    </wsdl:message>
    <wsdl:message name="BestOfferProcessResponseMessage">
        <wsdl:part name="payload" element="ns1:response"/>
    </wsdl:message>
    <wsdl:portType name="BestOfferProcess">
        <wsdl:operation name="process">
            <wsdl:input message="client:BestOfferProcessRequestMessage"/>
            <wsdl:output message="client:BestOfferProcessResponseMessage"/>
        </wsdl:operation>
    </wsdl:portType>
</wsdl:definitions>
